@model System.Web.Mvc.PagedList<PinheiroSereni.Negocio.Repositories.ControlPanel.CorretoraRepository>
@{
    ViewBag.Title = "Painel de Controle";
}
@{
    Layout= null;
}           

<div class="div-header-list">
    Cadastro de Corretoras
</div>
<div class="div-table-list">
    <table  id="table-sort" class="tablesorter" style="width: 100%">
        <thead>
            <tr>
                <th style="width: 100px"></th>
                <th style="width: 50px">
                    CÓDIGO
                </th>
                <th>
                    CORRETORA
                </th>
            </tr>
        </thead>
    @foreach (var item in Model)
    {
        <tbody>
        <tr id="linha" style="cursor: pointer;" onmouseover="javascript:TarjaIn(this);" onmouseout="javascript:TarjaOut(this);">
            <td>            
                @Ajax.ActionLink(" ", "Edit", "Corretoras", new { id = item.corretora.corretoraId }, new AjaxOptions { HttpMethod = "Post" }, new { @class = "link-editar", @title="Editar" })
                @Ajax.ActionLink(" ", "Delete", "Corretoras", new { id = item.corretora.corretoraId }, new AjaxOptions { HttpMethod = "Post" }, new { @class = "link-excluir", @title="Excluir" })
            </td>
            <td style="text-align: center">
                @item.corretora.corretoraId
            </td>
            <td>
                @item.corretora.nome
            </td>
        </tr>
	    </tbody>
    }

    </table>
    
    
    @if (Model.Count > 0)
    {
        @Html.Partial("Paginacao", Model)
    }
</div>
