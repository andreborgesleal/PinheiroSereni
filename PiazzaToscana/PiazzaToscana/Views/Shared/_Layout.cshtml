                </div>
                <!-- Fotos do empreendimento -->
                <div style="height: auto; margin-top: 0px; width: 650px; float: left;" >
                    <div style="width: auto; margin-top: 20px; margin-left: 0px; padding-right: 5px; float: left; cursor: pointer">
                        <img src="~/../../content/themes/base/images/PiazzaToscana/seta_esquerda.png" alt="" onmouseover="menos()" /> 
                    </div>
                    <div style="width: auto; margin-top: 0px; margin-left: 0px; float: left; cursor: pointer;">
                        <a id="aId" onclick="showPic(2)"><img id="imgId" src="~/../../content/themes/base/images/PiazzaToscana/Brinquedoteca.png" alt="" onmouseover="moldura('ON',this);" onmouseout="moldura('OFF',this);" /></a>
                    </div>
                    <div style="width: auto; margin-top: 0px; margin-left: 10px; float: left; cursor: pointer">
                        <a id="aId1" onclick="showPic(1)"><img id="imgId1" src="~/../../content/themes/base/images/PiazzaToscana/churrasqueira.png" alt="" onmouseover="moldura('ON',this);" onmouseout="moldura('OFF',this);" /></a>
                    </div>
                    <div style="width: auto; margin-top: 0px; margin-left: 10px; float: left; cursor: pointer">
                        <a id="aId2" onclick="showPic(0)"><img id="imgId2" src="~/../../content/themes/base/images/PiazzaToscana/cinema.png" alt="" onmouseover="moldura('ON',this);" onmouseout="moldura('OFF',this);" /></a>
                    </div>
                    <div style="width: auto; margin-top: 0px; margin-left: 10px; float: left; cursor: pointer">
                        <a id="aId3" onclick="showPic(-1)"><img id="imgId3" src="~/../../content/themes/base/images/PiazzaToscana/cyber.png" alt="" onmouseover="moldura('ON',this);" onmouseout="moldura('OFF',this);" /></a>
                    </div>
                    <div style="width: auto; margin-top: 20px; margin-right: 44px; float: right; cursor: pointer">
                        <img src="~/../../content/themes/base/images/PiazzaToscana/seta_direita.png" alt="" onmouseover="mais()" /> 
                    </div>
                </div>
            </div>
            <div style="width: 335px; margin-left: 0px; float: left; margin-top: -140px" >
                <div style="height: 314px; margin-top: 0px; margin-left: 13px; width: 330px; float: left; background-image: url(~/../../content/themes/base/images/PiazzaToscana/box3.png); background-repeat: no-repeat; font-size: 14px; color: #fff; font-weight: bold" >
                    <div style="width: 290px; height: 40px; padding-top: 20px; padding-left: 10px; padding-bottom: 10px; font-family: Arial; text-align: center">Inscreva-se aqui e receba, agora mesmo, o folder digital do Piazza Toscana.<p></p></div>
                     @RenderBody()
