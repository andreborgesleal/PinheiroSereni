</body>

<script type="text/javascript">
    array1 = new Array("~/../../content/themes/base/images/PiazzaToscana/Brinquedoteca.png",
                        "~/../../content/themes/base/images/PiazzaToscana/espmulher.png",
                        "~/../../content/themes/base/images/PiazzaToscana/churrasqueira.png",
                        "~/../../content/themes/base/images/PiazzaToscana/cinema.png",
                        "~/../../content/themes/base/images/PiazzaToscana/cyber.png",                        
                        "~/../../content/themes/base/images/PiazzaToscana/fitness.png",
                        "~/../../content/themes/base/images/PiazzaToscana/gourmet.png",
                        "~/../../content/themes/base/images/PiazzaToscana/hallsocial.png",
                        "~/../../content/themes/base/images/PiazzaToscana/piscina.png",
                        "~/../../content/themes/base/images/PiazzaToscana/playaventura.png",
                        "~/../../content/themes/base/images/PiazzaToscana/playbaby.png",
                        "~/../../content/themes/base/images/PiazzaToscana/pracacooper.png",
                        "~/../../content/themes/base/images/PiazzaToscana/pracadosdesejos.png",
                        "~/../../content/themes/base/images/PiazzaToscana/quadraesportiva.png",
                        "~/../../content/themes/base/images/PiazzaToscana/sacada.png",
                        "~/../../content/themes/base/images/PiazzaToscana/sala.png",
                        "~/../../content/themes/base/images/PiazzaToscana/salaofestas.png",
                        "~/../../content/themes/base/images/PiazzaToscana/salaofestasinfantil.png",
                        "~/../../content/themes/base/images/PiazzaToscana/squash.png",
                        "~/../../content/themes/base/images/PiazzaToscana/suite.png",
                        "~/../../content/themes/base/images/PiazzaToscana/jogos.png",
                        "~/../../content/themes/base/images/PiazzaToscana/jogosjuvenis.png")

    array2 = new Array("~/../../content/themes/base/images/PiazzaToscana/BrinquedotecaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/espmulherGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/churrasqueiraGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/cinemaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/cyberGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/fitnessGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/gourmetGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/hallsocialGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/piscinaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/playaventuraGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/playbabyGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/pracacooperGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/pracadosdesejosGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/quadraesportivaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/sacadaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/salaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/salaofestasGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/salaofestasinfantilGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/squashGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/suiteGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/jogosGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/jogosjuvenisGrande.png")

    function moldura(status,obj) {
        if (status == 'ON') {
            obj.style["border"] = "solid";
            obj.style["border-width"] = "2px";
            obj.style["border-color"] = "#243480";
            obj.style["border-radius"] = "5px";
        }
        else {
            obj.style["border"] = "none";
        }
    }

    function closeDetail(obj) {
        document.getElementById(obj).style["visibility"] = "hidden";
    }

    function showPic(position) {
        
        document.getElementById('divFotos').style['visibility'] = 'visible';
        var idx = parseInt(document.form.texto.value) - 1;
        var link = '@Url.Action("ShowPic")';
        link = encodeURI(link + '?img=' + array2[idx - position]);
        $('#divFotos').load(link);
    }

    function showMapa() {
        document.getElementById("divMapa").style["visibility"] = "visible";
    }
    function comeco() {
        
        document.getElementById('imgId').src = array1[0]
        document.getElementById('imgId1').src = array1[1]
        document.getElementById('imgId2').src = array1[2]
        document.getElementById('imgId3').src = array1[3]
        document.form.texto.value = "3"
    }

    function mais() {
        var index = parseInt(document.form.texto.value) ;
        if (index < 21) {
            if (index <= 17) 
                index = index + 4;
            else
                index = index + 1;
            document.getElementById('imgId').src = array1[index - 3]
            document.getElementById('imgId1').src = array1[index - 2]
            document.getElementById('imgId2').src = array1[index - 1]
            document.getElementById('imgId3').src = array1[index]

            document.form.texto.value = index.toString();
        }
    }

    function menos() {

        var index = parseInt(document.form.texto.value);
        if (index >= 4) {
            if (index >= 20)
                index = index - 1;
            else
                index = index - 4;
            document.getElementById('imgId').src = array1[index - 3]
            document.getElementById('imgId1').src = array1[index - 2]
            document.getElementById('imgId2').src = array1[index - 1]
            document.getElementById('imgId3').src = array1[index]

            document.form.texto.value = index.toString();
        }
    }

    function numeros() {

        if (document.all) // Internet Explorer  
            var tecla = event.keyCode;
        else if (document.layers) // Nestcape  
            var tecla = e.which;
        else if (document.getElementsByTagName("*"))
            var tecla = event.keyCode;
        if ((tecla > 47 && tecla < 58)) // numeros de 0 a 9  
            return true;
        else {
            if (tecla != 8) // backspace  
                //event.keyCode = 0;  
                return false;
            else
                return true;
        }
    }  

    function Mascara(objeto) {
        if (numeros(objeto)) {
            if (objeto.value.length == 0)
                objeto.value = '(' + objeto.value;

            if (objeto.value.length == 3)
                objeto.value = objeto.value + ')';

            if (objeto.value.length == 8)
                objeto.value = objeto.value + '-';
            return true;
        }
        else
            return false;
    }


    function Desabilita(id) {
        document.getElementById(id).disabled = "disabled";
        return true;
    }

</script>
</html>
