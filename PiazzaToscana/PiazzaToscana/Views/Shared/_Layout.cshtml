<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link rel="SHORTCUT ICON" href="/favicon.png" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/MicrosoftAjax.js")" type="text/javascript"></script>     
    <script src="@Url.Content("~/Scripts/MicrosoftMvcAjax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MicrosoftMvcValidation.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput-1.2.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>

</head>
<body style="background-color: #fff" onLoad="comeco()">
    <div class="page">
        <a id="lnkCorretorOnline" href="http://www.vivaciweb.com.br/chat/client.php?locale=pt-br&amp;group=1" target="_blank" style = "color: #000; text-decoration: none" onclick="if(navigator.userAgent.toLowerCase().indexOf('opera') != -1 &amp;&amp; window.event.preventDefault) window.event.preventDefault();this.newWindow = window.open(&#039;http://www.vivaciweb.com.br/chat/client.php?locale=pt-br&amp;group=1&amp;url=&#039;+escape(document.location.href)+&#039;&amp;referrer=&#039;+escape(document.referrer), 'mibew', 'toolbar=0,scrollbars=0,location=0,status=1,menubar=0,width=640,height=480,resizable=1');this.newWindow.focus();this.newWindow.opener=window;return false;"></a>  
        <div style="height: 132px; background-image: url(~/../../content/themes/base/images/NovoLayout/Apartamentos_Belem.png); background-repeat: no-repeat; width: 102%; margin-left: -2%">
            <div style="height: 122px; width: 280px; float: left; margin-left: 0px; font-size: 12px; color: #fff; text-align: right">
                &nbsp;
            </div>
            <div style="cursor: pointer; float: left; height: 132px; width: 295px; background-image: url(~/../../content/themes/base/images/NovoLayout/imoveis.png); background-repeat: no-repeat" onclick="javascript:contabilizaClick();" >
                &nbsp;
            </div>
            <div style="float: left; cursor: pointer; height: 132px; width: 250px; background-image: url(~/../../content/themes/base/images/NovoLayout/Apartamentos_a_venda.png); background-repeat: no-repeat; margin-top: 25px">
                &nbsp;
            </div>
            <div style="height: 132px; width: 150px; float: left; font-weight: bold; font-size: 11px; color: #fff; text-align: left; background-image: url(~/../../content/themes/base/images/NovoLayout/Apartamentos.png); background-repeat: no-repeat; margin-top: 35px">
                &nbsp;
            </div>
        </div>
        <div style="width: 987px; margin-left: 0px">
            <div style="margin-top: -30px; width: 555px; float: left">
                <div style="height: 343px; width: 555px; cursor: pointer; background-image: url(~/../../content/themes/base/images/NovoLayout/Financiamento_Imoveis.png); background-repeat: no-repeat; float: left">
                    &nbsp;
                </div>
            </div>
            <div style="margin-top: -70px; width: 425px; margin-left: 0px; float: left" >
                <div style="width: 350px; height: 210px; margin-top: 0px; background-image: url(~/../../content/themes/base/images/NovoLayout/Piazza_Toscana.png); background-repeat: no-repeat; background-position-x: 60px; background-position-y: 20px; float: left">
                </div>
                <div style="width: 350px; height: 264px; margin-top: -15px; background-image: url(~/../../content/themes/base/images/NovoLayout/Apartamentos_na_planta.png); background-repeat: no-repeat; background-position-x: 65px; background-position-y: 20px; float: left">
                    &nbsp;
                </div>
            </div>
        </div>
        <div style="width: 987px; margin-left: 0px">
            <div style="margin-top: -30px; width: 620px; float: left">
                <div style="margin-top: -5px; width: 650px; float: left;" >
                    <div style="height: 25px; float: left; width: 320px;">&nbsp;</div>
                    <div style="height: 25px; float: left; width: 270px; background-image: url(~/../../content/themes/base/images/NovoLayout/Imoveis_a_venda_belem.png); background-repeat: no-repeat" >
                        &nbsp;
                    </div>
                </div>
                <!-- Fotos do empreendimento -->
                <div style="height: auto; margin-top: 0px; width: 650px; float: left;" >
                    <div style="width: auto; margin-top: 20px; margin-left: 0px; padding-right: 5px; float: left; cursor: pointer">
                        <img src="~/../../content/themes/base/images/NovoLayout/Imovel_na_planta.png" alt="" onmouseover="menos()" /> 
                    </div>
                    <div style="width: auto; margin-top: 0px; margin-left: 0px; float: left; cursor: pointer;">
                        <a id="aId" onclick="showPic(2)"><img id="imgId" src="~/../../content/themes/base/images/PiazzaToscana/Brinquedoteca.png" alt="" onmouseover="moldura('ON',this);" onmouseout="moldura('OFF',this);" /></a>
                    </div>
                    <div style="width: auto; margin-top: 0px; margin-left: 10px; float: left; cursor: pointer">
                        <a id="aId1" onclick="showPic(1)"><img id="imgId1" src="~/../../content/themes/base/images/PiazzaToscana/churrasqueira.png" alt="" onmouseover="moldura('ON',this);" onmouseout="moldura('OFF',this);" /></a>
                    </div>
                    <div style="width: auto; margin-top: 0px; margin-left: 10px; float: left; cursor: pointer">
                        <a id="aId2" onclick="showPic(0)"><img id="imgId2" src="~/../../content/themes/base/images/PiazzaToscana/cinema.png" alt="" onmouseover="moldura('ON',this);" onmouseout="moldura('OFF',this);" /></a>
                    </div>
                    <div style="width: auto; margin-top: 0px; margin-left: 10px; float: left; cursor: pointer">
                        <a id="aId3" onclick="showPic(-1)"><img id="imgId3" src="~/../../content/themes/base/images/PiazzaToscana/cyber.png" alt="" onmouseover="moldura('ON',this);" onmouseout="moldura('OFF',this);" /></a>
                    </div>
                    <div style="width: auto; margin-top: 20px; margin-right: 44px; float: right; cursor: pointer">
                        <img src="~/../../content/themes/base/images/NovoLayout/Imovel_lançamento.png" alt="" onmouseover="mais()" /> 
                    </div>
                </div>
            </div>
            <div style="width: 335px; margin-left: 0px; float: left; margin-top: 0px" >
                <div style="height: 120px; margin-top: 0px; margin-left: 0px; width: 330px; float: left; background-image: url(~/../../content/themes/base/images/NovoLayout/Apartamentos_novos.png); background-repeat: no-repeat; font-size: 14px; color: #fff; font-weight: bold" >
                    <div style="width: 290px; height: 10px; padding-left: 10px; font-family: Arial; text-align: center"><p></p></div>
                     @RenderBody()
                </div>
            </div>
        </div>
        <!-- Google Map -->
        <div style="width: 670px; margin-top: -24px; float: left; font-size: 16px; color: #808080; font-family: Calibri; text-align: center; margin-left: 135px" >
            <div style="float: left; height: 30px; width: 670px; background-image: url(~/../../content/themes/base/images/PiazzaToscana/google_pin_min.png); background-repeat: no-repeat; background-position-x: 250px; margin-top: -20px; font-weight: bold; cursor: pointer" onclick="showMapa()">
                <div style="margin-top: 12px; width: 670px; padding-left: 15px">  
                    Mapa com a localização.
                </div>
            </div>
            <div style="height: 0px; margin-top: 0px; color: #808080; text-align: center; font-weight: normal; width: 670px; float: left; font-size: 11px" >
                vendaspiazzatoscana.com.br - todos os direitos reservados
            </div>
        </div>
        <form name="form" style="height: 0px">
            <input type="hidden" name="texto">
        </form>
        <!-- Fotos ampliadas do empreendimento -->
        <div id="divFotos" style="visibility: hidden; height: 0px" >
        </div>
        <div id="divMensagem" style="visibility: hidden; height: 0px">
            <div class="fundoModal">
            </div>
            <div class="modalPopup">
                <div>
                    @ViewBag.Mensagem
                    
                    <p></p>
                    <p></p>
                </div>
                <input type="button" value="Fechar" onclick="closeDetail(this)" />
            </div>
        </div>
        <div id="divMapa" style="visibility: hidden; height: 0px">
            <div class="fundoModal" onclick="closeDetail('divMapa')">
            </div>
            <div class="modalPopupMapa">
                <div id="printMap">
                    <img style="width: auto; height: auto" id="fotoMap" src="~/../../content/themes/base/images/PiazzaToscana/mapa.png" alt="texto"/>
                </div>
            </div>
        </div>
        <div id="carregando" style="visibility: hidden; height: 0px">
            <div class="fundoModal">
            </div>
            <div class="modalPopupCarregando">
                <div>
                    <img style="width: auto; height: auto" src="~/../../content/themes/base/images/PiazzaToscana/carregando.gif" alt="texto"/><br />
                </div>
            </div>
        </div>
        <div id="chatClick"></div>
        <section id="main">
           
        </section>
        <footer>
        </footer>
    </div>
</body>

<script type="text/javascript">
    array1 = new Array("~/../../content/themes/base/images/PiazzaToscana/Brinquedoteca.png",
                        "~/../../content/themes/base/images/PiazzaToscana/espmulher.png",
                        "~/../../content/themes/base/images/PiazzaToscana/churrasqueira.png",
                        "~/../../content/themes/base/images/PiazzaToscana/cinema.png",
                        "~/../../content/themes/base/images/PiazzaToscana/cyber.png",
                        "~/../../content/themes/base/images/PiazzaToscana/fitness.png",
                        "~/../../content/themes/base/images/PiazzaToscana/gourmet.png",
                        "~/../../content/themes/base/images/PiazzaToscana/hallsocial.png",
                        "~/../../content/themes/base/images/PiazzaToscana/piscina.png",
                        "~/../../content/themes/base/images/PiazzaToscana/playaventura.png",
                        "~/../../content/themes/base/images/PiazzaToscana/playbaby.png",
                        "~/../../content/themes/base/images/PiazzaToscana/pracacooper.png",
                        "~/../../content/themes/base/images/PiazzaToscana/pracadosdesejos.png",
                        "~/../../content/themes/base/images/PiazzaToscana/quadraesportiva.png",
                        "~/../../content/themes/base/images/PiazzaToscana/sacada.png",
                        "~/../../content/themes/base/images/PiazzaToscana/sala.png",
                        "~/../../content/themes/base/images/PiazzaToscana/salaofestas.png",
                        "~/../../content/themes/base/images/PiazzaToscana/salaofestasinfantil.png",
                        "~/../../content/themes/base/images/PiazzaToscana/squash.png",
                        "~/../../content/themes/base/images/PiazzaToscana/suite.png",
                        "~/../../content/themes/base/images/PiazzaToscana/jogos.png",
                        "~/../../content/themes/base/images/PiazzaToscana/jogosjuvenis.png")

    array2 = new Array("~/../../content/themes/base/images/PiazzaToscana/BrinquedotecaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/espmulherGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/churrasqueiraGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/cinemaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/cyberGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/fitnessGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/gourmetGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/hallsocialGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/piscinaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/playaventuraGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/playbabyGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/pracacooperGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/pracadosdesejosGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/quadraesportivaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/sacadaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/salaGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/salaofestasGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/salaofestasinfantilGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/squashGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/suiteGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/jogosGrande.png",
                    "~/../../content/themes/base/images/PiazzaToscana/jogosjuvenisGrande.png")

    function contabilizaClick() {
        $('#lnkCorretorOnline').click();
        var link = '@Url.Action("ContabilizaChat")';
        $('#chatClick').load(link);
    }

    function moldura(status, obj) {
        if (status == 'ON') {
            obj.style["border"] = "solid";
            obj.style["border-width"] = "2px";
            obj.style["border-color"] = "#243480";
            obj.style["border-radius"] = "5px";
            obj.style["opacity"] = "0.6";
        }
        else {
            obj.style["border"] = "none";
            obj.style["opacity"] = "1";
        }
    }

    function closeDetail(obj) {
        document.getElementById(obj).style["visibility"] = "hidden";
    }

    function showPic(position) {

        document.getElementById('divFotos').style['visibility'] = 'visible';
        var idx = parseInt(document.form.texto.value) - 1;
        var link = '@Url.Action("ShowPic")';
        link = encodeURI(link + '?img=' + array2[idx - position]);
        $('#divFotos').load(link);
    }

    function showMapa() {
        document.getElementById("divMapa").style["visibility"] = "visible";
    }
    function comeco() {

        document.getElementById('imgId').src = array1[0]
        document.getElementById('imgId1').src = array1[1]
        document.getElementById('imgId2').src = array1[2]
        document.getElementById('imgId3').src = array1[3]
        document.form.texto.value = "3"
    }

    function mais() {
        var index = parseInt(document.form.texto.value);
        if (index < 21) {
            if (index <= 17)
                index = index + 4;
            else
                index = index + 1;
            document.getElementById('imgId').src = array1[index - 3]
            document.getElementById('imgId1').src = array1[index - 2]
            document.getElementById('imgId2').src = array1[index - 1]
            document.getElementById('imgId3').src = array1[index]

            document.form.texto.value = index.toString();
        }
    }

    function menos() {

        var index = parseInt(document.form.texto.value);
        if (index >= 4) {
            if (index >= 20)
                index = index - 1;
            else
                index = index - 4;
            document.getElementById('imgId').src = array1[index - 3]
            document.getElementById('imgId1').src = array1[index - 2]
            document.getElementById('imgId2').src = array1[index - 1]
            document.getElementById('imgId3').src = array1[index]

            document.form.texto.value = index.toString();
        }
    }

    function numeros() {

        if (document.all) // Internet Explorer  
            var tecla = event.keyCode;
        else if (document.layers) // Nestcape  
            var tecla = e.which;
        else if (document.getElementsByTagName("*"))
            var tecla = event.keyCode;
        if ((tecla > 47 && tecla < 58)) // numeros de 0 a 9  
            return true;
        else {
            if (tecla != 8) // backspace  
                //event.keyCode = 0;  
                return false;
            else
                return true;
        }
    }

    function Mascara(objeto) {
        if (numeros(objeto)) {
            if (objeto.value.length == 0)
                objeto.value = '(' + objeto.value;

            if (objeto.value.length == 3)
                objeto.value = objeto.value + ')';

            if (objeto.value.length == 8)
                objeto.value = objeto.value + '-';
            return true;
        }
        else
            return false;
    }


    function Desabilita(id) {
        document.getElementById(id).disabled = "disabled";
        return true;
    }

</script>
</html>
	