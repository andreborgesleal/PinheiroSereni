        <!-- Google Map -->
        <div style="width: 670px; height: 42px; margin-top: -45px; float: left; font-size: 18px; color: #243480; font-family: Calibri; text-align: center" >
            <div style="float: left; height: 45px; width: 670px; background-image: url(~/../../content/themes/base/images/PiazzaToscana/google_pin.png); background-repeat: no-repeat; background-position-x: 215px; margin-top: -20px; font-weight: bold; cursor: pointer" onclick="showMapa()">
                <div style="margin-top: 19px; width: 670px">  
                    Mapa com a localização.
                </div>
            </div>
            <div style="height: 0px; margin-top: 0px; color: #000; text-align: center; font-weight: normal; width: 670px; float: left; font-size: 12px" >
                vendaspiazzatoscana.com.br - 2013 - todos os direitos reservados
            </div>
        </div>
        <form name="form" style="height: 0px">
            <input type="hidden" name="texto">
        </form>
        <!-- Fotos ampliadas do empreendimento -->
        <div id="divFotos" style="visibility: hidden; height: 0px" >
        </div>
        <div id="divMensagem" style="visibility: hidden; height: 0px">
            <div class="fundoModal">
            </div>
            <div class="modalPopup">
                <div>
                    @ViewBag.Mensagem
                    
                    <p></p>
                    <p></p>
                </div>
                <input type="button" value="Fechar" onclick="closeDetail(this)" />
            </div>
        </div>
        <div id="divMapa" style="visibility: hidden; height: 0px">
            <div class="fundoModal" onclick="closeDetail('divMapa')">
            </div>
            <div class="modalPopupMapa">
                <div id="printMap">
                    <img style="width: auto; height: auto" id="fotoMap" src="~/../../content/themes/base/images/PiazzaToscana/mapa.png" alt="texto"/>
                </div>
            </div>
        </div>
        <div id="carregando" style="visibility: hidden; height: 0px">
            <div class="fundoModal">
            </div>
            <div class="modalPopupCarregando">
                <div>
                    <img style="width: auto; height: auto" src="~/../../content/themes/base/images/PiazzaToscana/carregando.gif" alt="texto"/><br />
                </div>
            </div>
        </div>
        <section id="main">
           
        </section>
        <footer>
        </footer>
